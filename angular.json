{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "client-project": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/img"
            ],
            "styles": [
              "src/css/bootstrap.min.css",
              "src/font-awesome/css/font-awesome.css",
              "src/css/animate.css",
              "src/styles.css",
              "src/css/Custome_style.css",
              "src/css/normalize.css",
              "src/css/w3.css",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss"
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "src/js/bootstrap.min.js",
              "src/js/plugins/metisMenu/jquery.metisMenu.js",
              "src/js/plugins/slimscroll/jquery.slimscroll.min.js",
              "src/js/inspinia.js",
              "src/js/plugins/pace/pace.min.js",
              "src/js/plugins/flot/jquery.flot.js",
              "src/js/plugins/flot/jquery.flot.tooltip.min.js",
              "src/js/plugins/flot/jquery.flot.resize.js",
              "src/js/plugins/peity/jquery.peity.min.js",
              "src/js/demo/peity-demo.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "client-project:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "client-project:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "client-project:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "src/js/jquery-3.1.1.min.js",
              "src/js/bootstrap.min.js",
              "src/js/plugins/metisMenu/jquery.metisMenu.js",
              "src/js/plugins/slimscroll/jquery.slimscroll.min.js",
              "src/js/inspinia.js",
              "src/js/plugins/pace/pace.min.js",
              "src/js/plugins/flot/jquery.flot.js",
              "src/js/plugins/flot/jquery.flot.tooltip.min.js",
              "src/js/plugins/flot/jquery.flot.resize.js",
              "src/js/plugins/peity/jquery.peity.min.js",
              "src/js/demo/peity-demo.js",
              "src/js/plugins/jxcel.js",
              "https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"
            ],
            "styles": [
              "src/css/bootstrap.min.css",
              "src/font-awesome/css/font-awesome.css",
              "src/css/animate.css",
              "src/styles.css",
              "src/css/Custome_style.css",
              "src/css/normalize.css",
              "src/css/w3.css",
              "src/css/plugins/jxcel.css"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/img"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "client-project-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "client-project:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "client-project",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "css"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
